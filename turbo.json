{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**/*"]
		},
		"clean": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"nuke": {
			"cache": false,
			"dependsOn": ["clean"]
		}
	}
}
