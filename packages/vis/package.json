{
	"name": "storybook-addon-vis",
	"version": "0.6.2",
	"description": "Storybook Vitest Image Snapshot addon",
	"keywords": [
		"image-snapshot",
		"storybook-addons",
		"vitest",
		"visual-test"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/repobuddy/storybook-image-snapshot",
		"directory": "packages/vis"
	},
	"license": "MIT",
	"author": {
		"name": "Homa Wong",
		"email": "homawong@gmail.com",
		"url": "https://github.com/unional"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./vitest-setup": "./dist/vitest-setup.js",
		"./vitest-plugin": {
			"types": "./dist/vitest-plugin.d.ts",
			"import": "./dist/vitest-plugin.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src",
		"*.js",
		"*.d.ts"
	],
	"scripts": {
		"build": "tsup",
		"build:sb": "storybook build",
		"build:watch": "pnpm build --watch",
		"clean": "rimraf dist",
		"dev": "run-p build:watch \"sb --quiet\"",
		"nuke": "rimraf node_modules",
		"prerelease": "zx scripts/prepublish-checks.js",
		"sb": "storybook dev -p 6006",
		"test": "vitest run --coverage",
		"w": "vitest"
	},
	"dependencies": {
		"dedent": "^1.5.1",
		"is-ci": "^3.0.1",
		"mkdirp": "^3.0.1",
		"pathe": "^1.1.2",
		"pixelmatch": "^6.0.0",
		"rimraf": "^6.0.1",
		"type-plus": "^8.0.0-beta.6"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^8.4.2",
		"@storybook/addon-interactions": "^8.4.2",
		"@storybook/addon-links": "^8.4.2",
		"@storybook/blocks": "^8.4.2",
		"@storybook/experimental-addon-test": "^8.4.2",
		"@storybook/react": "^8.4.2",
		"@storybook/react-vite": "^8.4.2",
		"@storybook/test": "^8.4.2",
		"@types/is-ci": "^3.0.4",
		"@types/pixelmatch": "^5.2.6",
		"@types/react": "^18.2.65",
		"@types/react-dom": "^18.2.21",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitest/browser": "^2.1.4",
		"@vitest/expect": "^2.1.4",
		"boxen": "^8.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^8.4.2",
		"tsup": "^8.2.4",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"zx": "^7.2.3"
	},
	"peerDependencies": {
		"@types/pixelmatch": "^5.2.6",
		"@vitest/browser": "^2.1.3",
		"storybook": "^8.3.0",
		"vitest": "^2.1.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"bundler": {
		"exportEntries": [
			"src/index.ts",
			"src/vitest-setup.ts"
		]
	},
	"storybook": {
		"displayName": "Image Snapshot integration",
		"supportedFrameworks": [
			"react",
			"vue",
			"angular",
			"web-components",
			"ember",
			"html",
			"svelte",
			"preact",
			"react-native"
		],
		"icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
	}
}
